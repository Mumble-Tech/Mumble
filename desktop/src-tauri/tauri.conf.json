{
  "build": {
    "beforeDevCommand": "pnpm exec vite --clearScreen=false",
    "beforeBuildCommand": "pnpm exec vite build",
    "devPath": "http://localhost:1420",
    "distDir": "./dist"
  },
  "package": {
    "productName": "mumble",
    "version": "0.0.1"
  },
  "tauri": {
    "macOSPrivateApi": true,
    "allowlist": {
      "all": true,
      "protocol": {
        "assetScope": ["*"]
      },
      "os": {
        "all": true
      },
      "dialog": {
        "all": true,
        "ask": true,
        "save": true
      }
    },
    "bundle": {
      "active": true,
      "category": "Streaming",
      "copyright": "MumbleLLC",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico",
        "icons/logo_transparent.png",
        "icons/logo.png"
      ],
      "identifier": "com.tauri.dev",
      "longDescription": "A cross-platform streaming platform, for the streaming service Mumble",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "minimumSystemVersion": "10.15",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "Video Streaming made easy...",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
				"title": "Spacedrive",
				"width": 1200,
				"height": 725,
				"minWidth": 700,
				"minHeight": 500,
				"resizable": true,
				"fullscreen": false,
				"alwaysOnTop": false,
				"focus": false,
				"fileDropEnabled": false,
				"decorations": true,
				"transparent": true,
				"center": true
			}
    ]
  }
}
