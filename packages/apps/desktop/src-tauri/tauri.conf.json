{
  "build": {
    "beforeDevCommand": "pnpm exec vite --clearScreen=false --mode development",
    "beforeBuildCommand": "pnpm exec vite build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "Mumble",
    "version": "0.0.1"
  },
  "tauri": {
    "macOsPrivateApi": true,
    "allowlist": {
      "all": true
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.tauri.dev",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "minimumSystemVersion": "10.15", 
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
			{
				"title": "Mumble",
				"width": 1400,
				"height": 725,
				"minWidth": 700,
				"minHeight": 500,
				"resizable": true,
				"fullscreen": false,
				"alwaysOnTop": false,
				"focus": false,
				"visible": true,
				"decorations": true,
				"transparent": true,
				"center": true
			}
		]
  }
}
