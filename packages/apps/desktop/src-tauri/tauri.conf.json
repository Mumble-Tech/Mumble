{
  "package": {
    "productName": "Mumble",
    "version": "0.0.1"
  },
  "build": {
    "beforeDevCommand": "pnpm exec vite --clearScreen=false --mode development",
    "beforeBuildCommand": "pnpm exec vite build",
    "devPath": "http://localhost:1420",
    "distDir": "./dist"
  },
  "tauri": {
    "macOSPrivateApi": true,
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "MumbleLLC",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.tauri.dev",
      "longDescription": "Mumble is a streaming application for the mumble service.",
      "resources": [],
      "shortDescription": "Streaming made Easy",
      "targets": "all",
      "macOS": {
        "frameworks": [],
        "entitlements": null,
        "minimumSystemVersion": "10.15",
        "exceptionDomain": "",
        "signingIdentity": null
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "allowlist": {
      "all": true,
      "protocol": {
        "assetScope": ["*"]
      },
      "os": {
        "all": true
      },
      "dialog": {
        "all": true,
        "open": true,
        "save": true
      }
    },
    "windows": [
      {
        "title": "Mumble Streaming App",
        "width": 1400,
        "height": 725,
        "minWidth": 700,
        "minHeight": 500,
        "resizable": true,
        "alwaysOnTop": false,
        "focus": false,
        "visible": true,
        "fileDropEnabled": false,
        "decorations": true,
        "transparent": true,
        "center": true
      }
    ]
  }
}
